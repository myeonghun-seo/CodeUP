import java.util.*;

public class Main
{
	public static void main(String[] args)
	{
	    Scanner sc = new Scanner(System.in);
	    int a = sc.nextInt();
	    char c = 'A';
	    // 90 70 40 0
	    if(a >= 90) c = 'A';
	    else if(a >= 70) c = 'B';
	    else if(a >= 40) c = 'C';
	    else if(a < 40) c = 'D';
	    
		System.out.println(c);
	}

}
